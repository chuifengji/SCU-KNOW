/* components/CustomModal/CustomModal.wxss */
.modal-con { position: fixed; top: 0; bottom: 0; width: 100%; height: 100%; z-index: 999;text-align: center }
.modal-con .modal-mask { position: absolute; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0.6); z-index: 999; }
.modal-con .modal-mask .modal-box{position: absolute;width: 240px;height: 120px;background: white;transform:translate(-50%,-50%);top:50%;left:50%;border-radius: 20px;}
.modal-con .modal-mask .modal-box .model-textOne{position: absolute;font-size: 14px;top:15px;width: 240px;}
.modal-con .modal-mask .modal-box .model-textTwo{position: absolute;font-size: 14px;top: 40px;width: 240px;}
.modal-con .modal-mask .modal-box .model-reject{position: absolute;font-size: 12px;color: #4586D4;left: 25px;top: 72px;line-height: 35px;height: 35px;width: 70px;border-radius: 17.5px;border: 1px solid #4586D4;box-sizing: border-box;

}
.modal-con .modal-mask .modal-box .model-ok{position: absolute;font-size: 12px;width: 70px;height: 35px;background-color: #4586D4;color: white;
top: 72px;right: 25px;border-radius: 17.5px;line-height: 35px;}