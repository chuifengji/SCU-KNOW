.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  background-color: #f8f8f8;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:35rpx;
  letter-spacing:2px;
}
.feedback{
  position: absolute;
  left: 40rpx;
  z-index: 999;
  color: #333333;
  font-size:48rpx;
}
